{
  "name": "react-magma",
  "version": "2.0.0",
  "private": true,
  "homepage": "https://react-magma.cengage.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/cengage/react-magma.git"
  },
  "license": "MIT",
  "workspaces": [
    "website/*",
    "packages/*"
  ],
  "scripts": {
    "build": "lerna run build --concurrency=6 --no-progress",
    "build:docs": "lerna run build --scope react-magma-docs --no-progress",
    "build:landing": "node website/react-magma-landing/scripts/build.js",
    "build:lite": "lerna run build --ignore react-magma-docs --concurrency=6 --no-progress",
    "build:storybook": "storybook build",
    "changeset": "changeset add",
    "check:pkgs": "manypkg check",
    "clean": "lerna exec --scope react-magma-docs 'gatsby clean' && lerna clean --yes && rimraf node_modules",
    "clean:docs": "lerna exec --scope react-magma-docs 'gatsby clean' && lerna clean --yes --scope react-magma-docs ",
    "copy:readme": "copy README.md packages/react-magma-dom",
    "develop": "npm run docs",
    "docs": "lerna run --scope react-magma-docs develop",
    "fix:all": "npm run format && npm run lint:fix",
    "fix:pkgs": "manypkg fix",
    "format": "prettier --write --log-level warn \"**/src/**/*.{js,jsx,ts,tsx,mdx,json,html,css,yml,yaml,graphql}\"",
    "getAlias": "node ./packages/react-magma-dom/scripts/getAlias.js",
    "getVersion": "node ./packages/react-magma-dom/scripts/getVersion.js",
    "compiler-checks": "lerna run compiler-checks --concurrency=6 --no-progress",
    "prebuild": "lerna run compiler-checks && lerna run lint --concurrency=6 --no-progress",
    "pretest": "lerna run compiler-checks --concurrency=6 --no-progress",
    "postinstall": "npm run build:lite",
    "lint": "lerna run lint --concurrency=6 --no-progress",
    "lint:ci": "lerna run lint:check --concurrency=6 --no-progress",
    "lint:fix": "lerna run lint:fix --concurrency=6 --no-progress",
    "pkg": "manypkg run",
    "plop": "ts-node scripts/plopfile.ts",
    "prepare": "husky install",
    "publish": "npm run copy:readme && lerna publish",
    "release": "changeset publish",
    "storybook": "NODE_OPTIONS='--max-old-space-size=4096' storybook dev -p 6006",
    "test": "jest",
    "test-dom": "lerna run test --scope react-magma-dom -- --watch --coverage=false",
    "test-watch": "jest --watch",
    "upgrade:pkgs": "manypkg upgrade",
    "version:exit": "changeset pre exit",
    "version:pkgs": "changeset version",
    "version:prerelease": "changeset pre enter next"
  },
  "dependencies": {
    "@babel/cli": "7.27.2",
    "@babel/core": "7.27.4",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-export-default-from": "7.27.1",
    "@babel/plugin-proposal-export-namespace-from": "7.18.9",
    "@babel/plugin-proposal-object-rest-spread": "7.20.7",
    "@babel/plugin-proposal-throw-expressions": "7.27.1",
    "@babel/plugin-transform-runtime": "^7.27.4",
    "@babel/preset-env": "7.27.2",
    "@babel/preset-react": "7.27.1",
    "@babel/preset-typescript": "7.27.1",
    "@babel/runtime-corejs3": "^7.27.6",
    "@changesets/changelog-github": "0.5.1",
    "@changesets/cli": "2.29.4",
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@commitlint/is-ignored": "19.8.1",
    "@commitlint/load": "19.8.1",
    "@commitlint/parse": "19.8.1",
    "@commitlint/rules": "19.8.1",
    "@commitlint/top-level": "19.8.1",
    "@emotion/babel-plugin": "11.13.5",
    "@emotion/jest": "11.13.0",
    "@eslint/compat": "1.3.0",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.29.0",
    "@manypkg/cli": "0.24.0",
    "@storybook/addon-a11y": "9.0.12",
    "@storybook/addon-docs": "9.0.12",
    "@storybook/addon-webpack5-compiler-babel": "^3.0.6",
    "@storybook/react": "9.0.12",
    "@storybook/react-webpack5": "9.0.12",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/jest": "29.5.14",
    "@types/jest-axe": "3.5.9",
    "@types/lodash": "4.17.18",
    "@types/node": "24.0.3",
    "@types/react": "18.3.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "8.34.1",
    "@typescript-eslint/parser": "8.34.1",
    "assert": "^2.1.0",
    "autoprefixer": "10.4.21",
    "copy": "0.3.2",
    "core-js": "^3.43.0",
    "eslint": "9.29.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-define-config": "2.1.0",
    "eslint-import-resolver-typescript": "4.4.3",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jest": "28.14.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-mdx": "3.5.0",
    "eslint-plugin-prettier": "5.5.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-storybook": "^9.0.12",
    "eslint-plugin-unicorn": "59.0.1",
    "globals": "16.2.0",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "jest-axe": "10.0.0",
    "jest-cli": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "lerna": "8.2.2",
    "lint-staged": "16.1.2",
    "lodash": "4.17.21",
    "microbundle": "0.15.1",
    "node-plop": "0.32.0",
    "object.assign": "4.1.7",
    "postcss": "8.5.6",
    "prettier": "3.5.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-test-renderer": "^18.3.1",
    "rollup-plugin-postcss": "4.0.2",
    "shelljs": "0.10.0",
    "storybook": "9.0.12",
    "ts-node": "10.9.2",
    "tsdx": "0.14.1",
    "typedoc": "^0.28.5",
    "typescript": "5.8.3",
    "typescript-eslint": "8.34.1",
    "webpack-filter-warnings-plugin": "1.2.1"
  },
  "engines": {
    "node": ">=20.19.2",
    "npm": ">=10.8.2"
  },
  "overrides": {
    "storybook": "$storybook"
  }
}

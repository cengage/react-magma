{
  "name": "react-magma",
  "version": "2.0.0",
  "private": true,
  "homepage": "https://react-magma.cengage.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/cengage/react-magma.git"
  },
  "license": "MIT",
  "workspaces": [
    "website/*",
    "packages/*",
    "patterns/*"
  ],
  "scripts": {
    "build": "lerna run build --concurrency=6 --no-progress",
    "build:docs": "lerna run build --scope react-magma-docs --no-progress",
    "build:landing": "node website/react-magma-landing/scripts/build.js",
    "build:lite": "lerna run build --ignore react-magma-docs --concurrency=6 --no-progress",
    "build:storybook": "build-storybook",
    "changeset": "changeset add",
    "check:pkgs": "manypkg check",
    "check:spelling": "cspell --config=.cspell.json packages/**/*.{md,ts,js,mdx,tsx,jsx}",
    "clean": "lerna exec --scope react-magma-docs 'gatsby clean' && lerna clean --yes && rimraf node_modules",
    "clean:docs": "lerna exec --scope react-magma-docs 'gatsby clean' && lerna clean --yes --scope react-magma-docs ",
    "cm-retry": "git-cz --retry",
    "copy:readme": "copy README.md packages/react-magma-dom",
    "develop": "npm run docs",
    "docs": "lerna run --scope react-magma-docs develop",
    "fix:all": "npm run format && npm run lint:fix",
    "fix:pkgs": "manypkg fix",
    "format": "prettier --write --log-level warn \"**/src/**/*.{js,jsx,ts,tsx,mdx,json,html,css,yml,yaml,graphql}\"",
    "getAlias": "node ./packages/react-magma-dom/scripts/getAlias.js",
    "getVersion": "node ./packages/react-magma-dom/scripts/getVersion.js",
    "postinstall": "npm run build:lite",
    "lint": "eslint \"**/src/**/*.{js,jsx,ts,tsx,mdx}\"",
    "lint:ci": "eslint --max-warnings 0 \"**/src/**/*.{js,jsx,ts,tsx,mdx}\"",
    "lint:fix": "eslint --fix \"**/src/**/*.{js,jsx,ts,tsx,mdx}\"",
    "pkg": "manypkg run",
    "plop": "ts-node scripts/plopfile.ts",
    "prepare": "husky install",
    "publish": "npm run copy:readme && lerna publish",
    "release": "changeset publish",
    "storybook": "NODE_OPTIONS='--max-old-space-size=4096' start-storybook -p 6006",
    "test": "jest",
    "test-dom": "lerna run test --scope react-magma-dom -- --watch --coverage=false",
    "test-watch": "jest --watch",
    "upgrade:pkgs": "manypkg upgrade",
    "version:exit": "changeset pre exit",
    "version:pkgs": "changeset version",
    "version:prerelease": "changeset pre enter next"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@babel/cli": "^7.27.2",
    "@babel/core": "^7.27.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-export-default-from": "^7.27.1",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-proposal-throw-expressions": "^7.27.1",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@changesets/changelog-github": "0.5.1",
    "@changesets/cli": "2.29.4",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@commitlint/is-ignored": "^12.1.4",
    "@commitlint/load": "^12.1.4",
    "@commitlint/parse": "^12.1.4",
    "@commitlint/rules": "^12.1.4",
    "@commitlint/top-level": "^12.1.4",
    "@emotion/babel-plugin": "^11.13.5",
    "@emotion/jest": "^11.13.0",
    "@manypkg/cli": "^0.18.0",
    "@storybook/addon-a11y": "^6.5.16",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-controls": "^6.5.16",
    "@storybook/addon-docs": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-measure": "^6.5.16",
    "@storybook/addon-toolbars": "^6.5.16",
    "@storybook/addons": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@testing-library/dom": "^7.31.2",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.17.16",
    "@types/node": "^12.20.55",
    "@types/react": "17.0.85",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "autoprefixer": "^10.4.21",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-polished": "^1.1.0",
    "commitizen": "^4.3.1",
    "conventional-changelog-cli": "^2.2.2",
    "conventional-changelog-conventionalcommits": "^4.6.3",
    "copy": "^0.3.2",
    "core-js": "^3.42.0",
    "cspell": "^4.2.8",
    "cspell-dict-es-es": "^1.1.2",
    "cspell-dict-lorem-ipsum": "^1.1.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-cengage": "^2.1.4",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^24.7.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-mdx": "^3.4.1",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-unicorn": "^22.0.0",
    "husky": "^9.1.7",
    "jest": "^26.6.3",
    "jest-axe": "^5.0.1",
    "jest-cli": "^26.6.3",
    "jest-dom": "^4.0.0",
    "jest-extended": "^0.11.5",
    "jest-watch-typeahead": "^0.6.5",
    "lerna": "^8.2.2",
    "lint-staged": "^15.5.2",
    "lodash": "^4.17.21",
    "microbundle": "^0.13.3",
    "node-plop": "^0.32.0",
    "object.assign": "^4.1.7",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "regenerator-runtime": "^0.13.7",
    "rollup-plugin-postcss": "^4.0.2",
    "shelljs": "^0.10.0",
    "ts-node": "^9.1.1",
    "tsdx": "^0.14.1",
    "typedoc": "^0.19.2",
    "typescript": "^3.9.7",
    "webpack-filter-warnings-plugin": "1.2.1"
  },
  "engines": {
    "node": ">=18.20.4",
    "npm": ">=v10.7.0"
  }
}

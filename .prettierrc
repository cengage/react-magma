{
  "printWidth": 80,
  "tabWidth": 2,
  "arrowParens": "avoid",
  "singleQuote": true,
  "trailingComma": "es5",
  "semi": true,
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "endOfLine": "lf",
  "overrides": [
    {
      "files": [
        "*.md",
        "*.mdx"
      ],
      "options": {
        "proseWrap": "preserve"
      }
    }
  ]
}

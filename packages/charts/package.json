  "devDependencies": {
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "dom-to-image-more": "3.1.4",
    "identity-obj-proxy": "^3.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-magma-dom": "^4.10.0-next.3",
    "react-magma-icons": "^3.2.0"
  },
  "peerDependencies": {
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-magma-dom": "^4.10.0-next.3",
    "react-magma-icons": "^3.2.0"
  },
  "engines": {

      "require": "./dist/charts.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "dist/charts.js",
  "umd:main": "dist/charts.umd.js",
  "module": "dist/charts.modern.module.js",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
